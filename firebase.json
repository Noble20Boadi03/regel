{
  "hosting": {
    "public": "public",
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/services",
        "destination": "/services.html"
      },
      {
        "source": "/booking", 
        "destination": "/booking.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
   "firestore": {
    "rules": "firestore.rules"
  }
}